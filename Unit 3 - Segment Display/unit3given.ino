//remember to change # to the number of your digital pin for the segment display's LED
int pinA = #;
int pinB = #;
int pinC  = #;
int pinD = #;
int pinE = #;
int pinF = #;
int pinG = #;
int  D1 = #;



void setup() {                
// initialize  the digital pins as outputs.
    pinMode(pinA, OUTPUT);     
    pinMode(pinB,  OUTPUT);     
    pinMode(pinC, OUTPUT);     
    pinMode(pinD, OUTPUT);     
    pinMode(pinE, OUTPUT);     
    pinMode(pinF, OUTPUT);     
    pinMode(pinG,  OUTPUT);   
    pinMode(D1, OUTPUT);  
}

